[named]
enabled = true
filter = named
logpath = /var/log/syslog
maxretry = 5
bantime = 3600
action = iptables[name=Named, port=53, protocol=udp]


[bind9]
enabled = true
port = domain,953
protocol = udp
filter = bind9
logpath = /var/log/syslog
maxretry = 5
bantime = 3600

[vsftpd]
enabled = true
port = 21,20,990
filter = vsftpd
logpath = /var/log/vsftpd.log
maxretry = 5
findtime = 120   
bantime = 600   
